<h1>new post erb</h1>

<%= form_for @post, url: '/api/v1/posts', method: 'post', html: {multipart: true} do |f| %>
  <%= f.label :photo %>
  <%= f.file_field :photo %><br>
  <%= f.label :description %>
  <%= f.text_field :description %><br>
  <%= f.hidden_field :user_id, value: @id %>
  <%= f.submit %>
<% end %>
